{
    "name": "aksara",
    "version": "1.0.0",
    "description": "automate copywriter",
    "main": "server.js",
    "engines": {
        "node": "14.15.0"
    },
    "scripts": {
        "dev": "ts-node-dev --respawn --pretty --transpile-only src/server.ts",
        "lint-and-fix": "eslint . --ext .ts --fix",
        "start": "npm run build && set NODE_ENV=production && node dist/server.js",
        "build": "tsc -p ./",
        "heroku-postbuild": "tsc -p ./"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/magicwarms/aksara.git"
    },
    "keywords": [
        "copywrite",
        "aksara",
        "automate",
        "copywrite",
        "copywriting",
        "nodejs",
        "expressjs",
        "typescript"
    ],
    "author": "Andhana Utama",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/magicwarms/aksara/issues"
    },
    "homepage": "https://github.com/magicwarms/aksara#readme",
    "dependencies": {
        "@types/bcrypt": "^5.0.0",
        "@types/express-rate-limit": "^5.1.1",
        "@types/express-slow-down": "^1.3.0",
        "@types/jsonwebtoken": "^8.5.5",
        "@types/lodash": "^4.14.175",
        "@types/multer": "^1.4.7",
        "axios": "^0.21.4",
        "bcrypt": "^5.0.1",
